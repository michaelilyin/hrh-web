{
  "name": "hrh",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201 --proxy-config proxy.conf.js",
    "start:cloud": "ng serve --port 4201 --proxy-config proxy-cloud.conf.js",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint --typeCheck=true",
    "prettier": "prettier --check \"**/*.{ts, html, scss}\"",
    "prettier:fix": "prettier --write \"**/*.{ts, html, scss}\"",
    "e2e": "ng e2e",
    "dev:ssr": "ng run hrh:serve-ssr",
    "serve:ssr": "node dist/hrh/server/main.js",
    "build:ssr": "ng run hrh:app-shell:production && ng run hrh:server:production",
    "build:simple-server": "ng build --prod && webpack --config simple-server.webpack.js",
    "build:ci": "npm run build:simple-server",
    "prerender": "ng run hrh:prerender",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.7",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "~9.0.7",
    "@angular/compiler": "~9.0.7",
    "@angular/core": "~9.0.7",
    "@angular/forms": "~9.0.7",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "~9.0.7",
    "@angular/platform-browser-dynamic": "~9.0.7",
    "@angular/platform-server": "~9.0.7",
    "@angular/router": "~9.0.7",
    "@angular/service-worker": "~9.0.7",
    "@nguniversal/express-engine": "^9.0.2",
    "@types/luxon": "^1.24.0",
    "angular-oauth2-oidc": "^9.2.1",
    "express": "^4.15.2",
    "faker": "^4.1.0",
    "luxon": "^1.24.1",
    "md5": "^2.2.1",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.31",
    "ngx-progressbar": "^6.0.2",
    "ngx-skeleton-loader": "^1.2.7",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.7",
    "@angular/cli": "~9.0.7",
    "@angular/compiler-cli": "~9.0.7",
    "@angular/language-service": "~9.0.7",
    "@babel/core": "^7.9.6",
    "@nguniversal/builders": "^9.0.2",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-notes": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/angular": "^5.3.18",
    "@types/express": "^4.17.0",
    "@types/faker": "^4.1.10",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/md5": "^2.2.0",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.1.0",
    "codelyzer": "^5.1.2",
    "husky": "^4.2.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "^10.1.2",
    "prettier": "2.0.4",
    "protractor": "~5.4.3",
    "ts-loader": "^7.0.1",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && lint-staged"
    }
  }
}
